- var pageTitle = 'Software solutions';
<!DOCTYPE html>
include pug/layout/IE-HTML
html(lang="en")
    head
        include pug/layout/meta
        include pug/layout/fonts
        include pug/layout/styles
    body
        section.home-page
        nav.navbar.navbar-expand-lg
            .container-fluid
                a.navbar-brand(href='#') 
                    img.logo(src="images/logo.png", alt="Etolv logo")
                button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
                    span.navbar-toggler-icon
                #navbarSupportedContent.collapse.navbar-collapse
                    ul.navbar-nav.mx-auto.mb-2.mb-lg-0
                        li.nav-item
                            a.nav-link.active(aria-current='page', href='#') Home
                        li.nav-item
                            a.nav-link(href='#') About
                        li.nav-item
                            a.nav-link(href='#') Services
                        li.nav-item
                            a.nav-link(href='#') Product
                        li.nav-item
                            a.nav-link(href='#') Clients
                        li.nav-item
                            a.nav-link(href='#') Contacts
                        

        include pug/layout/scripts